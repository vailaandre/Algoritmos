/*Algoritmo que mostra na tela os números pares inteiros de 1 a 100*/

programa
{
	
	funcao inicio()
	{
		para(inteiro i= 1 ; i <=50 ; i=i+1){
			escreva("Número ", 2*i , "\n")
		}
	}
}
